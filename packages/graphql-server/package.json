{
    "name": "@boilerplate/graphql-server",
    "support": "https://via-profit.ru",
    "version": "1.0.1",
    "description": "Website boilerplate server",
    "engines": {
        "node": ">= 17.0.0",
        "npm": ">= 8.19.3"
    },
    "scripts": {
        "start": "npm run build:dev -- --watch",
        "start:dist": "npm run build:dist && node ./dist/index.js",
        "dist": "npm run lint && npm run build:dist",
        "build:dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack-config.ts --color --progress",
        "build:dist": "cross-env NODE_ENV=production webpack --config ./webpack/webpack-config.ts --color --progress",
        "build:migrations": "find  ./.knex/ -name '*.js' -delete && webpack --config ./webpack/webpack-migrations.ts",
        "analyze": "cross-env ANALYZE=true npm run build:dist",
        "migrate:latest": "knex migrate:latest --knexfile ./.knex/knexfile.js",
        "migrate:rollback": "knex migrate:rollback --knexfile ./.knex/knexfile.js",
        "migrate:rollback:all": "knex migrate:rollback --all --knexfile ./.knex/knexfile.js",
        "migrate:up": "knex migrate:up --knexfile ./.knex/knexfile.js",
        "migrate:down": "knex migrate:down --knexfile ./.knex/knexfile.js",
        "migrate:make": "knex migrate:make --migrations-directory ./src/database/migrations -x ts",
        "migrate:list": "knex migrate:list --knexfile ./.knex/knexfile.js",
        "migrate:check": "npm run migrate:list | grep -i -E '^found'",
        "seed:run": "knex seed:run --knexfile ./.knex/knexfile.js",
        "lint": "tsc --noEmit && eslint --fix .",
        "pretty": "prettier --write ./src",
        "deploy": "node ./scripts/deploy.js",
        "test": "cross-env TEST=true jest"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:via-profit-services/graphql-boilerplate.git"
    },
    "author": {
        "name": "Via Profit",
        "email": "promo@via-profit.ru",
        "url": "https://dev.via-profit.ru"
    },
    "contributors": [
        "Vasily Novosad <delhsmail@gmail.com>",
        "Pavel Natalin <trubonru@gmail.com>"
    ],
    "license": "MIT",
    "dependencies": {
        "@types/ws": "^8.5.4",
        "@via-profit-services/core": "^2.3.0",
        "@via-profit-services/geography": "^1.5.1",
        "@via-profit-services/permissions": "^2.0.3-rc.3",
        "bcryptjs": "^2.4.3",
        "bufferutil": "^4.0.7",
        "busboy": "^1.6.0",
        "cross-env": "^7.0.3",
        "dataloader": "^2.2.2",
        "dotenv": "^16.0.3",
        "graphql": "^16.6.0",
        "graphql-redis-subscriptions": "^2.6.0",
        "graphql-subscriptions": "^2.0.0",
        "graphql-ws": "^5.11.2",
        "iconv-lite": "^0.6.3",
        "ioredis": "^5.2.5",
        "jsonwebtoken": "^9.0.0",
        "knex": "^2.4.2",
        "pg": "^8.8.0",
        "utf-8-validate": "^6.0.1",
        "uuid": "^9.0.0",
        "ws": "^8.12.0"
    },
    "devDependencies": {
        "@jagi/jest-transform-graphql": "^1.0.2",
        "@jest/types": "^29.4.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/busboy": "^1.5.0",
        "@types/jest": "^29.2.6",
        "@types/jsonwebtoken": "^9.0.1",
        "@types/node": "^18.11.18",
        "@types/nodemon": "^1.19.2",
        "@types/prettier": "^2.7.2",
        "@types/uuid": "^9.0.0",
        "@types/webpack": "^5.28.0",
        "@types/webpack-bundle-analyzer": "^4.6.0",
        "@typescript-eslint/eslint-plugin": "^5.49.0",
        "@typescript-eslint/parser": "^5.49.0",
        "concurrently": "^8.0.1",
        "eslint": "^8.32.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-import-resolver-node": "^0.3.7",
        "eslint-import-resolver-webpack": "^0.13.2",
        "eslint-plugin-file-progress": "^1.3.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.32.1",
        "file-loader": "^6.2.0",
        "graphql-tag": "^2.12.6",
        "jest": "^29.4.0",
        "jest-transform-graphql": "^2.1.0",
        "node-loader": "^2.0.0",
        "nodemon": "^2.0.20",
        "nodemon-webpack-plugin": "^4.8.1",
        "prettier": "^2.8.3",
        "raw-loader": "^4.0.2",
        "supports-color": "^9.3.1",
        "ts-jest": "^29.0.5",
        "ts-loader": "^9.4.2",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.4",
        "webpack": "^5.75.0",
        "webpack-bundle-analyzer": "^4.7.0",
        "webpack-cli": "^5.0.1",
        "webpack-merge": "^5.8.0"
    }
}
